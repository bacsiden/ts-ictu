@model ts.ictu.mConstant
<h3>
    Html page</h3>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script>
    function getCkheight() {
        return '20em';
    }
</script>
<script src="/Content/ckeditor/ckeditor.js" type="text/javascript"></script>
@using (Html.BeginForm(null, null, FormMethod.Post, new { @class = "form-horizontal" }))
{
    @Html.ValidationSummary(true)                    

    @Html.HiddenFor(model => model.ID)
    <div class="control-group">
        <label for="Address">
            Code</label>
        <div class="">
            <div>
                @Html.TextBoxFor(model => model.KeyUrl, new { @class = "input-xxlarge" })
            </div>
        </div>
    </div>
    <div class="control-group">
        <label>
            Nội dung</label>
        <div class="">
            <textarea name="Content" rows="15" class="span10">@(Model != null ? Model.Content : null)</textarea>
            
        </div>
    </div>
    <div class="form-actions">
        @Html.Partial("_ShowSaveEdit", "")
    </div>
}
